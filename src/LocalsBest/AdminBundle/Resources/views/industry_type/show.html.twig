{% extends '::base.html.twig' %}

{% block content -%}
    <h1>IndustryType</h1>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_industry_type') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('admin_industry_type_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
