{% if 'jpeg' in file.fileName|lower or 'png' in file.fileName|lower or 'jpg' in file.fileName|lower %}
    <iframe srcdoc="<!DOCTYPE html><img src='{{ vich_uploader_asset(file, bucket) }}' style='width:100%;'/>" style="width: 100%; height: 650px;" frameborder="0"></iframe>
{% elseif 'pdf' in file.fileName|lower %}
    <iframe src="{{ vich_uploader_asset(file, bucket) }}" style="width: 100%; height: 650px;" frameborder="0"></iframe>
{% endif %}